{
    "enabledManagers": [
        "nuget"
    ],
    "extends": [
        "config:base"
    ],
    "nuget": {
        "enabled": true
    },
    "defaultRegistryUrls": [
        "https://api.nuget.org/v3/index.json"
    ],
    "packageRules": [
        {
            "matchRepositories": [
                "*"
            ],
            "matchUpdateTypes": [
                "minor",
                "patch"
            ]
        }
    ],
    "labels": [
        "Renovate-update-dependencies"
    ],
    "reviewers": [
        "[REVIEWER_EMAIL_ADDRESS_GOES_HERE]"
    ]
}