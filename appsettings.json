{
  "ElasticConfiguration": {
    "Uri": ""
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "AWSSDK": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "WithThreadId", "WithMachineName" ]
  },
  "StorageOptions": {
    "SourceDatabaseConnection": "",
    "TargetDatabaseConnection": "",
    "SourceDatabase": "test",
    "TargetDatabase": "t1d-monitoring",
    "CollectionNames": [
      "activity",
      "auth_roles",
      "auth_subjects",
      "devicestatus",
      "entries",
      "food",
      "profile",
      "settings",
      "treatments"
    ]
  }
}
